{
  "name": "docomo-utils",
  "version": "1.2.2",
  "description": "set of utilities function",
  "main": "dist/docomo-utils.js",
  "jsnext:main": "src/main.js",
  "module": "src5/main.js",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "transpile": "babel src --out-dir src5",
    "build": "NODE_ENV=production npm run clean && npm run build:dev && npm run build:prd && npm run transpile",
    "build:dev": "webpack dist/$npm_package_name.js --config tools/webpack.config.dev.js",
    "build:prd": "webpack dist/$npm_package_name.min.js --config tools/webpack.config.prd.js",
    "clean": "rimraf dist build docs src5",
    "lint": "eslint src",
    "preversion": "npm run test",
    "version": "npm run build && npm run documentation && git add -fA README.md src/ dist/ docs/",
    "test": "NODE_ENV=test karma start --single-run --no-auto-watch karma.config.js",
    "test:watch": "NODE_ENV=test karma start karma.config.js",
    "documentation": "npm run gen:docs && npm run gen:readme",
    "serve:docs": "./node_modules/documentation/bin/documentation.js serve src/main.js",
    "gen:docs": "./node_modules/documentation/bin/documentation.js build src/main.js -f html -o docs/$npm_package_version",
    "gen:readme": "./node_modules/documentation/bin/documentation.js readme src/main.js --section=API"
  },
  "keywords": [
    "es6",
    "babel",
    "webpack",
    "jasmine",
    "karma",
    "istanbul",
    "http",
    "client"
  ],
  "author": "Pasquale Mangialavori",
  "license": "MIT",
  "devDependencies": {
    "axios-mock-adapter": "^1.8.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^4.1.6",
    "babel-istanbul": "^0.6.0",
    "babel-istanbul-loader": "0.0.1",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.3.13",
    "documentation": "^4.0.0-beta16",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^3.1.0",
    "jasmine-core": "^2.4.1",
    "js-cookie": "^2.1.4",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-mocha-reporter": "^2.2.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.7.0",
    "loader-utils": "^0.2.12",
    "rimraf": "^2.5.0",
    "webpack": "^1.12.11"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-runtime": "^6.20.0"
  }
}
